@model IEnumerable<CourseProject.Models.SortedModels.AgentSorted>

<h2>Список агентов</h2>
<table class="table">
    <tr>
        <td>ФИО</td>
        <td>Тип оплаты</td>
        <td>Тип найма</td>
        <td></td>
    </tr>
    @foreach (var agent in Model)
    {
        <tr>
            <td>@agent.FirstName @agent.LastName</td>
            <td>@agent.Payment</td>
            <td>@agent.Type</td>
        </tr>
    }
</table>
<a asp-action="Create">Добавить агента</a>